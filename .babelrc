{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-typescript",
    "babel-preset-expo"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "navigation": "./src/navigation",
          "components/*": "./src/components/*",
          "HOCs": "./src/HOCs"
        }
      }
    ]
  ]
}
